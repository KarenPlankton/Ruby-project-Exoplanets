          <p><a href='/result_details/<%= planet.loc_rowid %>'>Details <%= planet.pl_name %></a></p>
          <p><a href='/update/<%= planet.loc_rowid %>'>Update <%= planet.pl_name %></a></p>
          <p><a href='/delete/<%= planet.loc_rowid %>'>Delete <%= planet.pl_name %></a></p>
          <hr />
        <% end %>


      </body>
