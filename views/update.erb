<td><input
type="text"
name="st_metratio"
id="st_metratio"
value="<%= @planet.st_metratio %>"
/></td>
</tr>
<tr>
<td><label>Stellar Age [Gyr]</label></td>
<td><input
type="text"
name="st_age"
id="st_age"
value="<%= @planet.st_age %>"
/></td>
</tr>
<tr>
<td><label>	Planet Equilibrium Temperature [K] [Gyr]</label></td>
<td><input
type="text"
name="pl_eqt"
id="pl_eqt"
value="<%= @planet.pl_eqt	 %>"
/></td>
</tr>

  <tr><td></td><td><input class="button" type="submit" ></td></tr>
</table>
</form>



