  <body>

    <form  method="post" action="/insert_action">
      <table>
        <tr>
          <td><label>Planet Name</label></td>
          <td><input
            type="text"
            name="pl_name"
            id="pl_name"
            value="<%= @planet.pl_name %>"
            /></td>
          </tr>
          <tr>
            <td><label>Host Star</label></td>
            <td><input
              type="text"
              name="pl_hostname"
              id="pl_hostname"
              value="<%= @planet.pl_hostname %>"
              /></td>
            </tr>
            <tr>
            <td><label>Discovery Method</label></td>
            <td><input
              type="text"
              name="pl_discmethod"
              id="pl_discmethod"
              value="<%= @planet.pl_discmethod %>"

              /></td>
            </tr>

          <tr>
          <td><label>Number of Planets in System</label></td>
          <td><input
            type="text"
            name="pl_pnum"
            id="pl_pnum"
            value="<%= @planet.pl_pnum %>"
            /></td>
          </tr>
          <tr>
        <td><label>Orbital Period(in days)</label></td>
        <td><input
          type="text"
          name="pl_orbper"
          id="pl_orbper"
          value="<%= @planet.pl_orbper %>"
          /></td>
        </tr>
      <tr>
