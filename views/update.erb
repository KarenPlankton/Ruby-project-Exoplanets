  <td><input
  type="text"
  name="pl_pelink"
  id="pl_pelink"
  value="<%= @planet.pl_pelink %>"
  /></td>
  </tr>
<tr>
<td><label>Link to Exoplanet Data Explorer</label></td>
<td><input
type="text"
name="pl_edelink"
id="pl_edelink"
value="<%= @planet.pl_edelink %>"
/></td>
</tr>
<tr>
<td><label>Spectral Type</label></td>
<td><input
type="text"
name="st_spstr"
id="st_spstr"
value="<%= @planet.st_spstr %>"
/></td>
</tr>
<tr>
<td><label>Stellar Surface Gravity</label></td>
<td><input
type="text"
name="st_logg"
id="st_logg"
value="<%= @planet.st_logg %>"
/></td>
</tr>
<tr>
<td><label>Stellar Luminosity [log(solar)</label></td>
<td><input
type="text"
name="st_lum"
id="st_lum"
value="<%= @planet.st_lum %>"
/></td>
</tr>
<tr>
<td><label>Stellar Surface Gravity</label></td>
<td><input
type="text"
name="st_logg"
id="st_logg"
value="<%= @planet.st_logg %>"
/></td>
</tr>
