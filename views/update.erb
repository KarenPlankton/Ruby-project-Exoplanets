    <td><input
      type="text"
      name="st_mass"
      id="st_mass"
      value="<%= @planet.st_mass %>"
      /></td>
    </tr>
  <tr>
  <td><label>Effective Temperature (K)</label></td>
  <td><input
    type="text"
    name="st_teff"
    id="st_teff"
    value="<%= @planet.st_teff %>"
    /></td>
  </tr>
  <tr>
  <td><label>Stellar Radius (solar radi)</label></td>
  <td><input
  type="text"
  name="st_rad"
  id="st_rad"
  value="<%= @planet.st_rad %>"
  /></td>
  </tr>
  <tr>
  <td><label>Planet Mass (Earth mass)</label></td>
  <td><input
  type="text"
  name="pl_masse"
  id="pl_masse"
  value="<%= @planet.pl_masse %>"
  /></td>
  </tr>
